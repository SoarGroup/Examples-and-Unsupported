source epmem_unit_common.soar

sp {apply*init2
    (state <s> ^operator.name init)
-->
    (<s> ^tests <tests>)
    (<tests> ^test <t-13>)
    (<t-13> ^id -1
            ^name |LTI constant|
            ^commands <c-13>
            ^result <r-13>)
    (<c-13> ^query.parity <parity>)
    (<r-13> ^graph-match 1
            ^match-cardinality 1
            ^memory-id 100)
}