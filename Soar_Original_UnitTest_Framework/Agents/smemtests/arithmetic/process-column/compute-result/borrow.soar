sp {compute-result*propose*borrow
   (state <s> ^name compute-result
             -^current-column.new-digit1 <r>
              ^result <r>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name borrow)
}

sp {compute-result*apply*borrow
   (state <s> ^operator <op>
              ^result <r>
              ^current-column <cc>)
   (<cc> ^digit1 <d1>)
   (<op> ^name borrow)
-->
   (<cc> ^new-digit1 <r>)
}

