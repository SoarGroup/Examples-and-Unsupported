chunk always
chunk only

watch --learn 2

sp {all*elaborate*name
   (state <s> ^superstate.operator.name <name>)
-->
   (<s> ^name <name>)
}

sp {all*elaborate*topstate
   (state <s> ^superstate.topstate <ts>)
-->
   (<s> ^topstate <ts>)
}

sp {all*elaborate*condition
   (state <s> ^superstate.condition <co>)
-->
   (<s> ^condition <co>)
}

sp {init*state
   (state <s> ^superstate nil)
-->
   (<s> ^object <N712> 
        ^topstate <s>
        ^condition <cond>)
   (<cond> ^name value ^arg.result <N712>)
   (<N712> ^id 6 ^type block ^value 5)
}

sp {turn-learning-on
(state <s> ^superstate.superstate nil)
-->
    (force-learn <s>)
    (<s> ^name learn-state)
}
#after state-no change impasse
sp {eval-condition*propose*condition-match
    (state <s1> ^superstate.name learn-state ^condition <c1> ^superstate <ss>)
    (<c1> ^name <name>) #CHANGE <v> to <word> to fix
    -->
    (<ss> ^operator <o1> + >)
    (<o1> ^name condition-match + ^type <name> +)
}

sp {condition-match*elaborate*matching-argument-object
(state <s> ^name condition-match
           ^topstate <ts>
           ^superstate <ss>)
    (<ss> ^condition <c> ^operator.type <type>) 
    (<c> ^arg.result <o>)
    (<o> ^<type> <v2>)
-->
    (<ts> ^result <r>)
    (<r> ^<random> <v2>)
}

sp {propose*stop
   (state <s> ^superstate.superstate.superstate.superstate nil)
-->
#    (success)
    (interrupt)
}