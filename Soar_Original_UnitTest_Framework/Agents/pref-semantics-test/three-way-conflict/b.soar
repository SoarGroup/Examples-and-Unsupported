sp {three-way-conflict*propose*b
   (state <s> ^name three-way-conflict)
-->
   (<s> ^operator <op> +)
   (<op> ^name b)
}

sp {three-way-conflict*compare*b
   (state <s> ^name three-way-conflict
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name b)
   (<op2> ^name c)
-->
   (<s> ^operator <op1> > <op2>)
}
