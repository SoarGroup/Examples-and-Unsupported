sp {three-way-conflict*propose*c
   (state <s> ^name three-way-conflict)
-->
   (<s> ^operator <op> +)
   (<op> ^name c)
}

sp {three-way-conflict*compare*c
   (state <s> ^name three-way-conflict
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name c)
   (<op2> ^name a)
-->
   (<s> ^operator <op1> > <op2>)
}
