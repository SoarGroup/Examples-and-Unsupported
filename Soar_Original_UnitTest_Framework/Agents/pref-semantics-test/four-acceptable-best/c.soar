sp {four-acceptable-best*propose*c
   (state <s> ^name four-acceptable-best)
-->
   (<s> ^operator <op> +)
   (<op> ^name c)
}

sp {four-acceptable-best*compare*c
   (state <s> ^name four-acceptable-best
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name c)
   (<op2> ^name a)
-->
   (<s> ^operator <op1> > <op2>)
}
