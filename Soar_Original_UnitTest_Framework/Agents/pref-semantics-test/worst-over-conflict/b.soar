sp {worst-over-conflict*propose*b
   (state <s> ^name worst-over-conflict)
-->
   (<s> ^operator <op> +)
   (<op> ^name b)
}

sp {worst-over-conflict*compare*b
   (state <s> ^name worst-over-conflict
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name b)
   (<op2> ^name a)
-->
   (<s> ^operator <op1> > <op2>)
}
