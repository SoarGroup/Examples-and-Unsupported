sp {three-way-two*propose*c
   (state <s> ^name three-way-two)
-->
   (<s> ^operator <op> +)
   (<op> ^name c)
}

sp {three-way-two*compare*c
   (state <s> ^name three-way-two
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name c)
   (<op2> ^name b)
-->
   (<s> ^operator <op1> < <op2>)
}
