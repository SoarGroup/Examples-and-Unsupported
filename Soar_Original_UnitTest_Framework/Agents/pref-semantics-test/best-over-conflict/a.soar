sp {best-over-conflict*propose*a
   (state <s> ^name best-over-conflict)
-->
   (<s> ^operator <op> +)
   (<op> ^name a)
}

sp {best-over-conflict*compare*a
   (state <s> ^name best-over-conflict
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name a)
   (<op2> ^name b)
-->
   (<s> ^operator <op1> > <op2>)
}
