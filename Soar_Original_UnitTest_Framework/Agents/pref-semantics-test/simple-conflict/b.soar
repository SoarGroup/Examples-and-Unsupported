sp {simple-conflict*propose*b
   (state <s> ^name simple-conflict)
-->
   (<s> ^operator <op> +)
   (<op> ^name b)
}

sp {simple-conflict*compare*b
   (state <s> ^name simple-conflict
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name b)
   (<op2> ^name a)
-->
   (<s> ^operator <op1> > <op2>)
}
