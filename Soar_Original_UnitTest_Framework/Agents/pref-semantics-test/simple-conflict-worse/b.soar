sp {simple-conflict-worse*propose*b
   (state <s> ^name simple-conflict-worse)
-->
   (<s> ^operator <op> + =)
   (<op> ^name b)
}

sp {simple-conflict-worse*compare*b
   (state <s> ^name simple-conflict-worse
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name b)
   (<op2> ^name a)
-->
   (<s> ^operator <op1> < <op2>)
}
